<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>KeepAlive</key>
	<dict>
		<key>Crashed</key>
		<true/>
		<key>SuccessfulExit</key>
		<true/>
	</dict>
	<key>Label</key>
	<string>cathy.samba-dot-org-smbd</string>
	<key>ProgramArguments</key>
	<array>
		<string>{{ template "brew-root" . }}/sbin/samba-dot-org-smbd</string>
		<string>-F</string>
		<string>--no-process-group</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
